#inckude<stdio.h>
#include<string.h>
void main ()
{
char s1[20],s2[20];
printf ("enter the strings");
gets(s1);
gets(s2);
int i,j,n,m,temp, count=0;
m=strlen(s1);
n=strlen(s2);
if(m==n)
{
for(i=0;i<n-1;i++)
{
for(j=0;j<n-i;j++)
{
if(s1[j]>s1[j+1])
{
temp=s1[j];
s1[j]=s1[j+1];
s1[j+1]=temp;
}
}
}
for(i=0;i<n-1;i++)
{
for(j=0;j<n-i;j++)
{
if(s2[j]>s2[j+1])
{
temp=s2[j];
s2[j]=s2[j+1];
s2[j+1]=temp;
}
}
}
for(i=0;i<n-1;i++)
{
if(s1[i]==s2[i])
count++;
}
if(count==n)
{
printf ("ANAGRAM");
else
printf ("NOT ANAGRAM");
}
} 
}
